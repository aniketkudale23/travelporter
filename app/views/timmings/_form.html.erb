<%= form_for @timming, :html => { :class => "form-horizontal timming" } do |f| %>

  <% if @timming.errors.any? %>
    <div id="error_expl" class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title"><%= pluralize(@timming.errors.count, "error") %> prohibited this timming from being saved:</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @timming.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :in, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :in, :class => 'form-control' %>
    </div>
    <%=f.error_span(:in) %>
  </div>
  <div class="form-group">
    <%= f.label :bus_type, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :bus_type, :class => 'form-control' %>
    </div>
    <%=f.error_span(:bus_type) %>
  </div>
  <div class="form-group">
    <%= f.label :provider_id, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :provider_id, :class => 'form-control' %>
    </div>
    <%=f.error_span(:provider_id) %>
  </div>

  <div class="form-group">
    <div class="col-lg-offset-2 col-lg-10">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                timmings_path, :class => 'btn btn-default' %>
    </div>
  </div>

<% end %>
