<%- model_class = Timming -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:dept_time) %>:</strong></dt>
  <dd><%= @timming.dept_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:bus_type) %>:</strong></dt>
  <dd><%= @timming.bus_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:provider_id) %>:</strong></dt>
  <dd><%= @timming.provider_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              timmings_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_timming_path(@timming), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              timming_path(@timming),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
